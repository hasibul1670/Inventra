{
  "name": "inventra-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "prisma:generate": "prisma generate --schema prisma/master/schema.prisma && prisma generate --schema prisma/tenant/schema",
    "prisma:migrate": "prisma migrate dev --schema prisma/master/schema.prisma && prisma migrate dev --schema prisma/tenant/schema",
    "prisma:generate:master": "prisma generate --schema prisma/master/schema.prisma",
    "prisma:generate:tenant": "prisma generate --schema prisma/tenant/schema",
    "prisma:migrate:master": "prisma migrate dev --schema prisma/master/schema.prisma",
    "prisma:migrate:tenant": "prisma migrate dev --schema prisma/tenant/schema"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@prisma/client": "^5.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
