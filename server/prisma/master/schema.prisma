generator client {
  provider = "prisma-client-js"
  output   = "../../src/prisma/generated/master"
}

datasource db {
  provider = "postgresql"
}

model Tenant {
  id        String   @id @default(uuid()) @db.Uuid
  slug      String   @unique
  domain    String?  @unique
  dbUrl     String
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now())
}
